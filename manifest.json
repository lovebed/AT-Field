{
    "name": "ATField",
    "version": "0.2.1",
    "description": "Anti-Tracking extension",
    "permissions": ["tabs", "webRequest", "webRequestBlocking", "http://*/*", "https://*/*"],
    "manifest_version": 2,
    "minimum_chrome_version": "18",

    "options_page": "frontend/options.html",
    "icons": {
        "128": "img/icon128.png",
        "16": "img/icon16.png",
        "48": "img/icon48.png"
    },

    "background": {
        "page": "background.html"
    },
    "browser_action": {
        "default_icon": "img/icon48.png",
        "default_popup": "frontend/popup.html"
    }
}